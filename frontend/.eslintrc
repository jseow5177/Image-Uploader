{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": ["plugin:prettier/recommended", "plugin:react/recommended"],
  "rules": {
    "no-unused-vars": 2,
    "no-undef": 2,
    "react/react-in-jsx-scope": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
